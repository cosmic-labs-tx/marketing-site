{
  "name": "starter",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "preview:wrangler": "wrangler pages dev ./dist",
    "astro": "astro",
    "prepare": "husky install",
    "lint": "eslint --cache --ext .js,.ts,.astro .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/partytown": "^2.0.1",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/sitemap": "^3.0.2",
    "@astrojs/tailwind": "^5.0.2",
    "@fontsource-variable/work-sans": "^5.0.14",
    "astro": "^3.5.5",
    "astro-google-fonts-optimizer": "^0.2.2",
    "astro-icon": "^1.0.0-next.2",
    "motion": "^10.16.4",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3"
  },
  "volta": {
    "node": "18.18.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.2.1",
    "@iconify-json/tabler": "^1.1.95",
    "@tailwindcss/forms": "^0.5.6",
    "@typescript-eslint/parser": "^6.8.0",
    "cypress": "^13.3.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "typescript": "^5.2.2",
    "wrangler": "^3.13.2"
  },
  "lint-staged": {
    "*.{js,ts,astro}": "eslint --cache --fix --ext .js,.ts,.astro",
    "*.{js,css,md,ts,astro}": "prettier --write"
  }
}
